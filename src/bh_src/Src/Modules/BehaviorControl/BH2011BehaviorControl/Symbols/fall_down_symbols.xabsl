
/** Symbols for setting the Fall-Down-State. */
namespace fall_down_symbols("Fall Down Symbols") {

  /** The ids of the states */
  enum fall_down_state {
    undefined, 
    upright, 
    on_ground,
	falling,
	staggering
  };

  /** The ids of the directions */
  enum fall_down_direction {
    none, 
    front, 
    left,
	back,
	right
  };

  enum fall_down_state input fall_down_state;
  enum fall_down_direction input fall_down_direction;
}
